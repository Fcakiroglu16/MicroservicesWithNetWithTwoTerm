{
  "ReverseProxy": {
    "Routes": {

      "microservice1-route": {
        "ClusterId": "microservice1-cluster",
        "Match": {
          "Path": "microservice1/{version}/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "api/{version}/{**catch-all}" } ]
      }
    },

    "Clusters": {
      "microservice1-cluster": {
        "LoadBalancingPolicy": "RoundRobin",
        "Destinations": {
          "microservice1-destination": {
            "Address": "http://dns or ip:5195"
          },
          "microservice1-destination2": {
            "Address": "http://dns or ip:5035"
          }
        }
      }



    }

  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
